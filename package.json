{
  "name": "@anchan828/typeorm-helpers",
  "private": true,
  "scripts": {
    "postinstall": "husky install && npm run bootstrap && npm run build",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "lint": "lerna run --parallel lint",
    "lint:fix": "lerna run --parallel lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:minor": "lerna publish --yes minor",
    "publish:major": "lerna publish --yes major",
    "test": "lerna run test && npx @anchan828/merge-coverage",
    "test:e2e": "lerna run test:e2e"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/jest": "26.0.24",
    "@types/node": "16.3.2",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "fast-glob": "3.2.7",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lerna": "4.0.0",
    "lint-staged": "11.1.1",
    "mysql": "2.18.1",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "typeorm": "0.2.35",
    "typescript": "4.3.5"
  }
}
