{
  "name": "@anchan828/typeorm-helpers",
  "private": true,
  "scripts": {
    "postinstall": "husky install && npm run bootstrap && npm run build",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "lint": "lerna run --parallel lint",
    "lint:fix": "lerna run --parallel lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:minor": "lerna publish --yes minor",
    "publish:major": "lerna publish --yes major",
    "test": "lerna run test && npx @anchan828/merge-coverage",
    "test:e2e": "lerna run test:e2e"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.3.2",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "fast-glob": "3.2.7",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lerna": "4.0.0",
    "lint-staged": "11.2.3",
    "mysql": "2.18.1",
    "pg": "8.7.1",
    "sqlite3": "5.0.2",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.3.0",
    "typeorm": "0.2.38",
    "typescript": "4.4.4"
  }
}
