{
  "name": "@anchan828/typeorm-helpers",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "postinstall": "husky install",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:major": "lerna publish --yes major",
    "publish:minor": "lerna publish --yes minor",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@lerna-lite/cli": "^2.5.0",
    "@lerna-lite/publish": "^2.5.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fast-glob": "^3.3.1",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "lint-staged": "^14.0.0",
    "mysql": "^2.18.1",
    "pg": "^8.11.3",
    "prettier": "^3.0.2",
    "sqlite3": "^5.1.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "turbo": "^1.10.12",
    "typeorm": "^0.3.17",
    "typescript": "^5.1.6"
  }
}
