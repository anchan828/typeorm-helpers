{
  "name": "@anchan828/typeorm-helpers",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "format": "prettier --write '**/*.{js,json,yml,yaml,md}'",
    "postinstall": "husky install",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "publish": "lerna publish --yes patch",
    "publish:major": "lerna publish --yes major",
    "publish:minor": "lerna publish --yes minor",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e"
  },
  "devDependencies": {
    "@commitlint/cli": "18.1.0",
    "@commitlint/config-conventional": "18.1.0",
    "@lerna-lite/cli": "2.6.0",
    "@lerna-lite/publish": "2.6.0",
    "@types/jest": "29.5.6",
    "@types/node": "20.8.8",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "fast-glob": "3.3.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.0.2",
    "mysql": "2.18.1",
    "pg": "8.11.3",
    "prettier": "3.0.3",
    "sqlite3": "5.1.6",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "turbo": "1.10.16",
    "typeorm": "0.3.17",
    "typescript": "5.2.2"
  }
}
